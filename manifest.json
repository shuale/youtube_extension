{
    "manifest_version": 3,
    "name": "YouTube Deaddicitifier",
    "version": "1.0",
    "permissions": ["storage"],
    "icons": {
        "16" : "icon.png",
        "32" : "icon.png",
        "48" : "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://m.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ]
}