{
    "manifest_version": 3,
    "name": "YouTube Browser Extension",
    "version": "1.0",
    "permissions": ["storage"],
    "icons": {
        "16" : "YouTube_browser_extension_icon.png",
        "32" : "YouTube_browser_extension_icon.png",
        "48" : "YouTube_browser_extension_icon.png",
        "128": "YouTube_browser_extension_icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://m.youtube.com/*"
            ],
            "js": ["content.js"]
        }
    ]
}